    "build": ":",
    "test": "jest --coverage",
    "watch": "jest --watch",
    "release": ":"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prettier": "^3.1.0"
  },
  "devDependencies": {
    "jest": "^25.3.0"
